<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">

		<flow name="get:\artists:xxx-music-factory-api-2-config">
        <set-variable value="artists-main::impl-get-artists" doc:name="currentFlow" doc:id="1344cac1-7aaf-417e-a842-281f72803b72" variableName="currentFlow"/>
		<flow-ref doc:name="impl-get-artists" doc:id="2d4eceaf-07cc-4756-a613-0a2b2797da77" name="impl-get-artists"/>
    </flow>
	<flow name="post:\artists:application\json:xxx-music-factory-api-2-config">
        <set-variable value="artists-main::impl-add-artists" doc:name="currentFlow" doc:id="974a0144-f826-4529-bc31-6ce2c5207c7f" variableName="currentFlow"/>
		<flow-ref doc:name="impl-add-artists" doc:id="73379d6f-18fe-4550-8a2a-1106b42fb048" name="impl-add-artists"/>
    </flow>
	<flow name="patch:\artists\(artistId):application\json:xxx-music-factory-api-2-config">
        <set-variable value="artists-main::impl-update-artists" doc:name="currentFlow" doc:id="16c5bc8f-839c-4f7f-a8e1-20a93cb53bd4" variableName="currentFlow"/>
		<flow-ref doc:name="impl-update-artists-urls" doc:id="7b56225d-d26e-4a58-bbd8-2bd50dcb1293" name="impl-update-artists-urls" />
		<ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core">
            <ee:variables>
                <ee:set-variable variableName="artistId">attributes.uriParams.'artistId'</ee:set-variable>
            </ee:variables>
        </ee:transform>
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
            <ee:message>
                <ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  statusCode: 201,
  message: "SUCCESS",
  bioUrl: "https://last.fm/music/The Beatles/+wiki",
  recordsUpdated: 3
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
    </flow>
	<flow name="get:\artists\(artistId):xxx-music-factory-api-2-config">
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core">
            <ee:variables>
                <ee:set-variable variableName="artistId">attributes.uriParams.'artistId'</ee:set-variable>
            </ee:variables>
        </ee:transform>
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
            <ee:message>
                <ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  statusCode: 200,
  message: "SUCCESSFUL",
  artist: {
    groupName: "Grateful Dead",
    firstName: "Garcia",
    lastName: "Gerry",
    dateOfBirth: "1948-01-07"
  }
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
    </flow>
	<flow name="put:\artists\(artistId):application\json:xxx-music-factory-api-2-config">
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core">
            <ee:variables>
                <ee:set-variable variableName="artistId">attributes.uriParams.'artistId'</ee:set-variable>
            </ee:variables>
        </ee:transform>
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
            <ee:message>
                <ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  statusCode: 200,
  message: "The artist has been updated."
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
    </flow>
    <flow name="delete:\artists\(artistId):xxx-music-factory-api-2-config">
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core">
            <ee:variables>
                <ee:set-variable variableName="artistId">attributes.uriParams.'artistId'</ee:set-variable>
            </ee:variables>
        </ee:transform>
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
            <ee:message>
                <ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  statusCode: 200,
  message: "The artist has been deleted."
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
    </flow>
	<flow name="get:\artists\(artistId)\songs:xxx-music-factory-api-2-config">
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core">
            <ee:variables>
                <ee:set-variable variableName="artistId">attributes.uriParams.'artistId'</ee:set-variable>
            </ee:variables>
        </ee:transform>
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
            <ee:message>
                <ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  statusCode: 200,
  message: "SUCCESSFUL",
  artistSongs: [
    {
      songTitle: "Hey Jude",
      publicationDate: "1968-08-26",
      genre: "Rock",
      artist: {
        groupName: "The Beatles",
        firstName: "John",
        lastName: "Lennon"
      }
    }, 
    {
      songTitle: "Magical Mystery Tour",
      publicationDate: "1967-03-15",
      genre: "Rock",
      artist: {
        groupName: "The Beatles",
        firstName: "John",
        lastName: "Lennon"
      }
    }
  ]
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
    </flow>




</mule>
